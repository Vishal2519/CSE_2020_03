{
    "manifest_version":2,
    "name": "basics",
    "version":"1",
    
    "content_scripts": [
        {
          "matches": [ "<all_urls>"],
          "js": ["content.js"]
        }
      ],
      "browser_action":{
        "default_popup":"popup.html"
    },

    "background":{
      "scripts":["background.js"]
    },

    "permissions":["tabs","tts"]



}
