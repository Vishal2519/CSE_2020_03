{
    "manifest_version": 2,
    "name": "GDE",
    "version": "2",
    "icons": {
        "16": "img/developers-logo.png",
        "48": "img/developers-logo.png",
        "128": "img/developers-logo.png"
    },
    "permissions": [
        "background",
        "identity",
        "notifications",
        "storage",
        "alarms",
        "https://www.googleapis.com/*",
        "https://*.googleusercontent.com/*",
        "tts",
        "ttsEngine"
    ],
    "background": {
        "scripts": [
            "js/background.js","js/textTS.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "img/gmail-logo.png",
            "38": "img/gmail-logo.png"
        },
        "default_title": "Click to Sign in with Google"
    },
    "oauth2": {
        "client_id": "702316745646-jkef39ome26s7kf481p0cg0idfla3ofc.apps.googleusercontent.com",
        "scopes": [
            "profile",
            "https://www.googleapis.com/auth/gmail.readonly"
        ]
    },
    "key": ""
}
